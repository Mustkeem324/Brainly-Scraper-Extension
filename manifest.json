{
    "manifest_version": 3,
    "name": "Brainly Scraper Extension",
    "version": "1.0",
    "description": "A simple extension to open links in incognito mode.",
    "permissions": [
      "contextMenus",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  